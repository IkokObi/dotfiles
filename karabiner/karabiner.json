            "description": "yen symbol to backslach",
            "manipulators": [
              {
                "from": {
                  "key_code": "international3"
                },
                "to": [
                  {
                    "key_code": "international3",
                    "modifiers": [
                      "option"
                    ]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "set vk2 is 1 when lang2 is down",
            "manipulators": [
              {
                "from": {
                  "key_code": "lang2",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "set_variable": {
                      "name": "vk2",
                      "value": 1
                    }
                  }
                ],
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "vk2",
                      "value": 0
                    }
                  }
                ],
                "to_if_alone": [
