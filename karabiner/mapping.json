{
  "title": "custom",
  "rules": [
    {
      "description": "set vk2 is 1 when lang2 is down",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "lang2",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "vk2",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "vk2",
                "value": 0
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "lang2"
            }
          ]
        }
      ]
    },
    {
      "description": "set h to left when vk2 is 1",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "type": "variable_if",
              "name": "vk2",
              "value": 1
            }
          ],
          "from": {
            "key_code": "h",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "set j to down when vk2 is 1",
